<!DOCTYPE html>
<html >
  <head>
    <meta charset="UTF-8">
    <title>Job Item</title>
    <link rel='stylesheet prefetch' href='http://maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css'>
  </head>
  <body>
    <div ng-app="myApp" ng-controller="MyCtrl">
      {{loading}}
      <div class="code">Code: <span>{{item.code}}</span></div>
      <div class="title">{{item.title}}</div>
      <div class="description">{{item.description}}</div>
      <div class="type">Type: <span>{{item.type}}</span></div>
      <div class="travel">travel: <span>{{item.travel}}</span></div>
      <div class="salary">salary: <span>{{item.salary}}</span></div>
      <div class="city">city: <span>{{item.city}}</span></div>
      <div class="state">state: <span>{{item.state}}</span></div>
      <div class="postalCode">zipcode: <span>{{item.postalCode}}</span></div>
      <div class="country">country: <span>{{item.country}}</span></div>
    </div>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.0/angular.min.js'></script>
    <script src="/js/item.js"></script>
  </body>
</html>
